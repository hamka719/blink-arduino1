int led0 = 3;
int led1 = 4;
int led2 = 5;
int led3 = 6;

void setup()
{
pinMode(3, OUTPUT);
pinMode(4, OUTPUT);
pinMode(5, OUTPUT);
pinMode(6, OUTPUT);
}

void loop()
{
digitalWrite(led0, HIGH);
delay(666);
digitalWrite(led0. LOW);
delay(666);
{
digitalWrite(led1, HIGH);
delay(666);
digitalWrite(led1, LOW);
delay(666);
}
{
digitalWrite(led2, HIGH);
delay(666);
digitalWrite(led2, LOW);
delay(666)
}
{
digitalWrite(led3, HIGH);
delay(666);
digitalWrite(led3, HIGH);
delay(666);
}
